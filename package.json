    "@effect/cli": "^0.73.0",
    "@effect/vitest": "^0.21.0",
    "msgpackr": "^1.11.4",
    "multipasta": "^0.2.7",
    "ai": "^6.0.39",
    "@ai-sdk/anthropic": "^2.0.0",
