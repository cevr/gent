{
  "name": "gent",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "type": "module",
  "scripts": {
    "typecheck": "tsc -b",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "test": "bun --filter '*' test",
    "gate": "bun run fmt:check && bun run lint && bun run typecheck",
    "clean": "rm -rf packages/*/dist apps/*/dist .tsbuildinfo */.tsbuildinfo */*/.tsbuildinfo",
    "smoke": "bun run --cwd apps/tui dev -H 'Say hi in 3 words'",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^4.0.29",
    "@aws-sdk/credential-providers": "^3.974.0",
    "@effect/cluster": "^0.56.1",
    "@effect/experimental": "^0.58.0",
    "@effect/rpc": "^0.73.0",
    "@effect/sql": "^0.49.0",
    "msgpackr": "^1.11.8",
    "multipasta": "^0.2.7"
  },
  "devDependencies": {
    "@ai-sdk/google": "^3.0.13",
    "@ai-sdk/mistral": "^3.0.12",
    "@effect/language-service": "^0.72.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-bun": "^0.87.1",
    "@effect/vitest": "^0.27.0",
    "@types/bun": "^1.3.6",
    "effect": "^3.19.15",
    "lefthook": "^2.0.15",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "catalog": {
    "effect": "^3.19.14",
