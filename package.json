{
  "name": "gent",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "type": "module",
  "scripts": {
    "typecheck": "turbo run typecheck",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "test": "turbo run test",
    "gate": "concurrently -n type,lint,fmt,test -c blue,yellow,magenta,green \"bun run typecheck\" \"bun run lint\" \"bun run fmt\" \"bun run test\"",
    "clean": "rm -rf .turbo */.turbo */*/.turbo",
    "smoke": "bun run --cwd apps/tui dev -H 'Say hi in 3 words'",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^4.0.45",
    "@aws-sdk/credential-providers": "^3.980.0",
    "@effect/cluster": "^0.56.1",
    "@effect/experimental": "^0.58.0",
    "@effect/rpc": "^0.73.0",
    "@effect/sql": "^0.49.0",
    "effect-machine": "^0.2.4",
    "msgpackr": "^1.11.8",
    "multipasta": "^0.2.7"
  },
  "devDependencies": {
    "@ai-sdk/google": "^3.0.19",
    "@ai-sdk/mistral": "^3.0.17",
    "@effect/language-service": "^0.72.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-bun": "^0.87.1",
    "@effect/vitest": "^0.27.0",
    "@types/bun": "^1.3.8",
    "concurrently": "^9.2.1",
    "effect": "^3.19.15",
    "lefthook": "^2.0.16",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "turbo": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "bun@1.3.6",
  "catalog": {
    "effect": "^3.19.14",
    "@effect/platform": "^0.94.1",
    "@effect/platform-bun": "^0.87.0",
    "@effect/sql": "^0.49.0",
    "@effect/sql-pg": "^0.50.1",
    "@effect/sql-sqlite-bun": "^0.50.0",
    "@effect/experimental": "^0.58.0",
    "@effect/rpc": "^0.73.0",
    "@effect/cluster": "^0.56.0",
    "@effect/cli": "^0.73.1",
    "@effect/vitest": "^0.21.5",
    "msgpackr": "^1.11.4",
    "multipasta": "^0.2.7",
    "ai": "^6.0.49",
    "@ai-sdk/anthropic": "^3.0.23",
    "@ai-sdk/openai": "^3.0.18",
    "@ai-sdk/google": "^3.0.13",
    "@ai-sdk/mistral": "^3.0.12",
    "vitest": "^3.2.4",
    "effect-machine": "^0.2.3"
  }
}
